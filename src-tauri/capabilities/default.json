{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "core:webview:allow-set-webview-zoom",
    "dialog:default",
    "opener:default",
    "os:default",
    "fs:allow-app-write-recursive",
    "fs:allow-app-read-recursive",
    "log:default"
  ]
}